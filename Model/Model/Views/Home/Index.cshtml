@model Model.Models.Pracownik

@{
    ViewBag.Title = "Index";
}

<h2>Index</h2>


<table>
    <tr>
        <td>Pracownik ID:</td>
        <td>@Model.PracownikId</td>
    </tr>
    <tr>
        <td>Imię pracownika</td>
        <td>@Model.Imie</td>
    </tr>
    <tr>
        <td>Nazwisko pracownika</td>
        <td>@Model.Nazwisko</td>
    </tr>
    <tr>
        <td>Miasto</td>
        <td>@Model.Miasto</td>
    </tr>
    <tr>
        <td>Ulica</td>
        <td>@Model.Ulica</td>
    </tr>
</table>
